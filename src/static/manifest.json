{
  "manifest_version": 3,
  "name": "Domscriptions",
  "version": "3.0.2",
  "description": "Easily mark (without hiding!) your YouTube subscriptions.",
  "homepage_url": "https://github.com/bcanseco/domscriptions",
  "author": "Borja Canseco",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/feed/subscriptions"],
      "js": ["domscriptions.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  }
}
