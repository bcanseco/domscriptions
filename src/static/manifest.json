{
  "manifest_version": 2,
  "name": "Domscriptions",
  "version": "2.1.0",
  "description": "Easily mark (without hiding!) your YouTube subscriptions.",
  "homepage_url": "https://github.com/bcanseco/domscriptions",
  "author": "Borja Canseco",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/feed/subscriptions"],
      "js": ["domscriptions.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_security_policy": "default-src 'self' 'sha256-bMYZhq6T+I552N1BDcysg+GYCkrI3LumlL8lyCTClEU=' 'sha256-QkzKF2npu0x6NR3DbkkP8gvWhaUUakzUyLUuiK5V7Qo=';"
}
